{
  "extends": "@tsconfig/svelte/tsconfig.json",

  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"],
  "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": false,
      "target": "es2016",
      "resolveJsonModule": true,
      "importsNotUsedAsValues": "remove"
  }
}