{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "cross-env ROLLUP_WATCH=1 postcss src/tailwind.pcss -o public/build/tailwind.css -w",
    "build:tailwind": "postcss src/tailwind.pcss -o public/build/tailwind.css",
    "build": "npm run build:tailwind && rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-wasm": "6.2.2",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.5",
    "@tsconfig/svelte": "5.0.2",
    "@types/human-date": "^1.4.0",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "rollup": "4.9.2",
    "rollup-plugin-css-only": "4.5.2",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.1.6",
    "sirv-cli": "2.0.2",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "svelte-preprocess": "5.1.3",
    "tailwindcss": "3.2.7",
    "tslib": "2.5.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "dexie": "3.2.4",
    "human-date": "1.4.0",
    "isomorphic-fetch": "3.0.0",
    "localforage": "1.10.0",
    "nz-search": "file:../nz-search/public/pkg",
    "ol": "8.2.0",
    "proj4": "2.9.2",
    "svelte-frappe-charts": "1.10.0",
    "uuid": "9.0.0"
  }
}
