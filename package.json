{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "cross-env ROLLUP_WATCH=1 postcss src/tailwind.pcss -o public/build/tailwind.css -w",
    "build:tailwind": "postcss src/tailwind.pcss -o public/build/tailwind.css",
    "build": "npm run build:tailwind && rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/human-date": "^1.4.0",
    "@types/ol": "^6.4.2",
    "autoprefixer": "^10.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.35.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.50.1",
    "svelte-check": "^1.1.24",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dexie": "^3.2.2",
    "human-date": "^1.4.0",
    "localforage": "^1.9.0",
    "ol": "^6.5.0",
    "svelte-frappe-charts": "^1.2.0",
    "uuid": "^9.0.0"
  }
}
